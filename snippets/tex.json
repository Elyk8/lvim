{
	"inlineeq": {
		"prefix": "mk",
		"body": "\\( $1 \\)$0",
		"description": "inline equation"
	},
	"fulleq": {
		"prefix": "dm",
		"body": "\\[\n\t$1\n\\]\n$0",
		"description": "inline equation"
	},
	"lessorequal": {
		"prefix": "<=",
		"body": "\\leq $0",
		"description": "less or equal"
	},
	"greaterorequal": {
		"prefix": ">=",
		"body": "\\geq $0",
		"description": "greater or equal"
	},
	"limits": {
		"prefix": "lim",
		"body": "\\lim_{$1 \to $2} $0",
		"description": "greater or equal"
	},
	"square": {
		"prefix": "sq",
		"body": "^2$0",
		"description": "inline equation"
	},
	"squareroot": {
		"prefix": "sr",
		"body": "\\sqrt{$1}$0",
		"description": "inline equation"
	},
	"product": {
		"prefix": "xx",
		"body": "\\times $0",
		"description": "product"
	},
	"sectionlabel": {
		"prefix": "sec",
		"body": "\\section{$1}%\n\\label{sec:${2:$1}} $0",
		"description": "Section"
	},
	"subsectionlabel": {
		"prefix": "sub",
		"body": "\\subsection{$1}%\n\\label{sec:${2:$1}} $0",
		"description": "Subsection"
	},
	"subsubsectionlabel": {
		"prefix": "ssub",
		"body": "\\subsubsection{$1}%\n\\label{sec:${2:$1}} $0",
		"description": "Subsubsection"
	}
}
